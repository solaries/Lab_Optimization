                            <a href="javascript:void(0);" class="has-arrow" aria-expanded="false"><i class="icon-notebook menu-icon"></i><span class="nav-text">Patient tests</span></a>
                            <ul aria-expanded="false">

                                @if (strRightList.IndexOf("sphinxcol" + "new_Patient_Tests" + "sphinxcol") > -1)
