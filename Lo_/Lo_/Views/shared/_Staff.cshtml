                                    <li><a href="@Url.Content("~/Staff/view_Test_Type/")">View</a></li>
                                }
                            </ul>
                        </li>
                    }

                    @if (strRightList.IndexOf("sphinxcol" + "new_Patient" + "sphinxcol") > -1
                                || strRightList.IndexOf("sphinxcol" + "view_Patient" + "sphinxcol") > -1)
                    {
                        <li>
                            <a href="javascript:void(0);" class="has-arrow" aria-expanded="false"><i class="icon-notebook menu-icon"></i><span class="nav-text">Patient</span></a>
