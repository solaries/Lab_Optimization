                    }

                    <li>
                        <a href="@Url.Content("~/Staff/Change_Password")">
                            <i class="icon-grid menu-icon"></i><span class="nav-text">Change password</span>
                        </a>
                    </li>
                    <li>
                        <a href="@Url.Content("~/Staff/login")">
                            <i class="icon-logout"></i><span class="nav-text">Logout</span>
                        </a>
                    </li>
                </ul>
            </div>
        </div>
        <div class="content-body">
            <div class="container-fluid">
                <div class="row justify-content-center">
                    <div class="col-lg-12">
                        @RenderBody()
                    </div>
                </div>
            </div>
        </div>
        <div class="footer">
            <div class="copyright">
            </div>
        </div>
    </div>
    <script src="~/assets/plugins/common/common.min.js"></script>
    <script src="~/assets/js/custom.min.js"></script>
    <script src="~/assets/js/settings.js"></script>
    <script src="~/assets/js/gleek.js"></script>
    <script src="~/assets/js/styleSwitcher.js"></script>
    <script src="~/assets/plugins/validation/jquery.validate.min.js"></script>
    <script src="~/assets/plugins/tables/js/jquery.dataTables.min.js"></script>
    <script src="~/assets/plugins/tables/js/datatable/dataTables.bootstrap4.min.js"></script>
    <script src="~/assets/plugins/validation/jquery.validate-init.js"></script>
    <script src="~/assets/plugins/tables/js/datatable-init/datatable-basic.min.js"></script>
    <script src="~/assets/plugins/bootstrap-datepicker/bootstrap-datepicker.min.js"></script>
    <script>

        jQuery('#Date_of_birth').datepicker({
            autoclose: true,
            format: 'dd/M/yyyy',
            todayhighlight: true 
        });

        jQuery('#Test_date').datepicker({
            autoclose: true,
            format: 'dd/M/yyyy',
            todayhighlight: true
        });

        jQuery('#Move_date').datepicker({
            autoclose: true,
            format: 'dd/M/yyyy',
            todayhighlight: true
        });</script>
